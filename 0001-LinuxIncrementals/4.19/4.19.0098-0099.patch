diff --git a/drivers/i2c/busses/i2c-stm32f7.c b/drivers/i2c/busses/i2c-stm32f7.c
index 144f30513fcb..e6bbb8ca7090 100644
--- a/drivers/i2c/busses/i2c-stm32f7.c
