diff --git a/fs/btrfs/send.c b/fs/btrfs/send.c
index 10532463e92a..931a7d1ddc95 100644
--- a/fs/btrfs/send.c
+++ b/fs/btrfs/send.c
@@ -6638,12 +6638,6 @@ long btrfs_ioctl_send(struct file *mnt_file, struct btrfs_ioctl_send_args *arg)
