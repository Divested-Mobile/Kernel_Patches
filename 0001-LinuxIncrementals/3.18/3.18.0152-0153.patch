index bd09807b6b05..cbb5008c65ab 100644
--- a/drivers/spi/spi-orion.c
+++ b/drivers/spi/spi-orion.c
@@ -112,37 +112,62 @@ static int orion_spi_baudrate_set(struct spi_device *spi, unsigned int speed)
