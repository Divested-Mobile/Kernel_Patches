--- a/drivers/net/vxlan.c
+++ b/drivers/net/vxlan.c
@@ -2462,6 +2462,8 @@ static void vxlan_uninit(struct net_device *dev)
 {
 	struct vxlan_dev *vxlan = netdev_priv(dev);
 
+	gro_cells_destroy(&vxlan->gro_cells);
