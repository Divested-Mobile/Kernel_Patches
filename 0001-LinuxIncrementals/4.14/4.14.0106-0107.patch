 	vxlan_fdb_delete_default(vxlan, vxlan->cfg.vni);
 
 	free_percpu(dev->tstats);
@@ -3523,7 +3525,6 @@ static void vxlan_dellink(struct net_device *dev, struct list_head *head)
 
